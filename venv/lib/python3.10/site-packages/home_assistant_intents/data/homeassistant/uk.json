    "intents": {
      "HassGetState": {
        "one": "{{ slots.name | capitalize }} {{ state.state_with_unit }}\n",
        "one_yesno": "{% if query.matched %}\n  Так\n{% else %}\n  Ні, {{ state.state_with_unit }}\n{% endif %}\n",
        "any": "{% if query.matched %}\n  {% set match = query.matched | map(attribute=\"name\") | sort | list %}\n  {% if match | length > 4 %}\n    Так, {{ match[:3] | join(\", \") }} і ще {{ (match | length - 3) }}\n  {%- else -%}\n    Так,\n    {% for name in match -%}\n      {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} and {% endif -%}\n      {{ name }}\n    {%- endfor -%}\n  {% endif %}\n{% else %}\n  Ні\n{% endif %}\n",
        "all": "{% if not query.unmatched: %}\n  Так\n{% else %}\n  {% set no_match = query.unmatched | map(attribute=\"name\") | sort | list %}\n  {% if no_match | length > 4 %}\n    Ні, {{ no_match[:3] | join(\", \") }} і ще {{ (no_match | length - 3) }} ні\n  {%- else -%}\n    Ні,\n    {% for name in no_match -%}\n      {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} and {% endif -%}\n      {{ name }}\n    {%- endfor %} ні\n  {% endif %}\n{% endif %}\n",
        "which": "{% if not query.matched %}\n  таких немає\n{% else: %}\n  {% set match = query.matched | map(attribute=\"name\") | sort | list %}\n  {% if match | length > 4 %}\n    {{ match[:3] | join(\", \") }} і ще {{ (match | length - 3) }}\n  {% else %}\n    {%- for name in match -%}\n      {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} і {% endif -%}\n      {{ name }}\n    {%- endfor -%}\n  {% endif %}\n{% endif %}\n",
        "how_many": "{{ query.matched | length }}\n"
      },

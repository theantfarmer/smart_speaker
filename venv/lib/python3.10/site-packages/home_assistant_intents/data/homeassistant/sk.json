{
  "language": "sk",
  "intents": {
    "HassTurnOff": {
      "data": [
        {
          "sentences": [
            "<turn_off> (ventilátor|vetrák) [(v|vo)] <area>"
          ],
          "slots": {
            "domain": "fan"
          },
          "response": "fan"
        },
        {
          "sentences": [
            "<turn_off> ( ventiláciu|vetranie) [(v|vo)] <area>"
          ],
          "slots": {
            "domain": "fan",
            "name": "all"
          },
          "response": "fans_ventilation"
        },
        {
          "sentences": [
            "<turn_off> [(všetky|všetko)] (ventilátory|vetráky|ventiláciu|vetranie) [(v|vo)] <area>"
          ],
          "slots": {
            "domain": "fan",
            "name": "all"
          },
          "response": "fans_area"
        },
        {
          "sentences": [
            "<turn_off> <name>"
          ]
        },
        {
          "sentences": [
            "<close> {name}"
          ],
          "response": "cover"
        },
        {
          "sentences": [
            "<close> {name} [v|vo|na|pred|za|pod|pred] {area}"
          ],
          "response": "cover_area"
        },
        {
          "sentences": [
            "(<turn_off>|<turn_off_light>) (svetlo|lampu) <area>"
          ],
          "slots": {
            "domain": "light"
          },
          "response": "light"
        },
        {
          "sentences": [
            "(<turn_off>|<turn_off_light>) [(všetky|všetko)] [(svetlá|osvetlenie|lampy)] [(v|vo|na|pred|pod|pri)] <area>"
          ],
          "slots": {
            "domain": "light",
            "name": "all"
          },
          "response": "lights_area"
        },
        {
          "sentences": [
            "<close> garáž",
            "<close> garážovú bránu"
          ],
          "response": "cover_device_class",
          "slots": {
            "device_class": "garage",
            "domain": "cover"
          }
        },
        {
          "sentences": [
            "<close> (rolety|žalúzie|záves[y]|záclon(u|y)) [v|na] {area}"
          ],
          "response": "cover_area",
          "slots": {
            "device_class": [
              "blind",
              "curtain",
              "shutter"
            ],
            "domain": "cover"
          }
        }
      ]
    },
    "HassGetState": {
      "data": [
        {
          "sentences": [
            "<what_is> <name> [<in> <area>]"
          ],
          "response": "one"
        },
        {
          "sentences": [
            "<is> <name> {on_off_states:state} [<in> <area>]"
          ],
          "response": "one_yesno",
          "excludes_context": {
            "domain": [
              "cover"
            ]
          }
        },
        {
          "sentences": [
            "<is> {on_off_states:state} <any> {on_off_domains:domain} [<in> <area>]"
          ],
          "response": "any"
        },
        {
          "sentences": [
            "<is> <all> {on_off_domains:domain} [<in> <area>] {on_off_states:state}"
          ],
          "response": "all"
        },
        {
          "sentences": [
            "<which> {on_off_domains:domain} <is> {on_off_states:state} [<in> <area>]"
          ],
          "response": "which"
        },
        {
          "sentences": [
            "<how_many> {on_off_domains:domain} <is> {on_off_states:state} [<in> <area>]"
          ],
          "response": "how_many"
        }
      ]
    },
    "HassTurnOn": {
      "data": [
        {
          "sentences": [
            "(<turn_on>|<turn_on_light>) (svetlo|lampu) <area>"
          ],
          "slots": {
            "domain": "light"
          },
          "response": "light"
        },
        {
          "sentences": [
            "(<turn_on>|<turn_on_light>) [(všetky|všetko)] [(svetlá|osvetlenie|lampy)] [(v|vo|na|pred|pod|pri)] <area>"
          ],
          "slots": {
            "domain": "light",
            "name": "all"
          },
          "response": "lights_area"
